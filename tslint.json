{
  "extends": [
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-use-before-declare": false,
    "strict-type-predicates": false,
    "no-floating-promises": false,
    "variable-name": [
      false,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
